{
  "name": "cards-generator",
  "version": "1.0.0",
  "description": "Generator for https://maif.github.io/cards",
  "main": "generator.js",
  "license": "Apache-2.0",
  "scripts": {
    "server": "node src/server.js",
    "generator": "node src/generator.js",
    "migrate": "node src/migrate.js",
    "translate": "node src/translate.js",
    "ooops": "node src/oooops.js",
    "pdf": "yarn generator && node src/pdf.js",
    "prettier": "prettier --write --single-quote --trailing-comma es5 --print-width 100 --tab-width 2 --jsx-bracket-same-line \"*.js\""
  },
  "dependencies": {
    "@google-cloud/translate": "1.1.0",
    "chrome-headless-render-pdf": "1.8.0",
    "express": "4.16.2",
    "fs-extra": "5.0.0",
    "nodemon": "1.18.9",
    "prettier": "1.11.1",
    "rimraf": "2.6.2",
    "showdown": "1.8.6"
  }
}
